name: workflow-ci

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: string, default: "None"}
      dataset: {type: string, default: "diabetes_dataset_2019_preprocessing.csv"}
    command: "python modelling.py {n_estimators} {max_depth} {dataset}"
