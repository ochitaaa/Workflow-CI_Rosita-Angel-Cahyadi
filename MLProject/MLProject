  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: null}
      dataset: {type: string, default: "Credit Score Dataset_preprocessing.csv"}
    command: "python modelling.py {n_estimators} {max_depth} {dataset}"
